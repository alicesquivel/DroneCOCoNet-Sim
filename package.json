    "@adonisjs/session": "^1.0.19",
    "@adonisjs/shield": "^1.0.4",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "axios": "^0.19.2",
    "chart.js": "^2.9.3",
